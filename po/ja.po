#: ../gnulib/lib/gai_strerror.c:44
msgid "Address family for hostname not supported"
msgstr "ホスト名に対するアドレス・ファミリ指定はサポートしていません"

#: ../gnulib/lib/gai_strerror.c:45
msgid "Temporary failure in name resolution"
msgstr "名前解決が一時的に失敗しました"

#: ../gnulib/lib/gai_strerror.c:46
msgid "Bad value for ai_flags"
msgstr "ai_flags の値が間違っています"

#: ../gnulib/lib/gai_strerror.c:47
msgid "Non-recoverable failure in name resolution"
msgstr "名前解決で復旧不可のエラーになりました"

#: ../gnulib/lib/gai_strerror.c:48
msgid "ai_family not supported"
msgstr "ai_family はサポートしていません"

#: ../gnulib/lib/gai_strerror.c:49
msgid "Memory allocation failure"
msgstr "メモリ確保エラーです"

#: ../gnulib/lib/gai_strerror.c:50
msgid "No address associated with hostname"
msgstr "ホスト名に関連づけられたアドレスはありません"

#: ../gnulib/lib/gai_strerror.c:51
msgid "Name or service not known"
msgstr "名前またはサービスが不明です"

#: ../gnulib/lib/gai_strerror.c:52
msgid "Servname not supported for ai_socktype"
msgstr "ai_socktype に対する Servname はサポートしていません"

#: ../gnulib/lib/gai_strerror.c:53
msgid "ai_socktype not supported"
msgstr "ai_socktype はサポートしていません"

#: ../gnulib/lib/gai_strerror.c:54
msgid "System error"
msgstr "システム・エラーです"

#: ../gnulib/lib/gai_strerror.c:55
msgid "Argument buffer too small"
msgstr "引数を受け取るバッファが小さすぎます"

#: ../gnulib/lib/gai_strerror.c:57
msgid "Processing request in progress"
msgstr "要求された処理を実行中です"

#: ../gnulib/lib/gai_strerror.c:58
msgid "Request canceled"
msgstr "要求がキャンセルされました"

#: ../gnulib/lib/gai_strerror.c:59
msgid "Request not canceled"
msgstr "要求はキャンセルされませんでした"

#: ../gnulib/lib/gai_strerror.c:60
msgid "All requests done"
msgstr "全ての要求に対する処理が完了しました"

#: ../gnulib/lib/gai_strerror.c:61
msgid "Interrupted by a signal"
msgstr "シグナルによる割り込みが発生しました"

#: ../gnulib/lib/gai_strerror.c:62
msgid "Parameter string not correctly encoded"
msgstr "引数として渡した文字列のエンコーディングが間違っています"

#: ../gnulib/lib/gai_strerror.c:74
msgid "Unknown error"
msgstr "原因不明のエラーです"

#: ../src/vncdisplay.c:150
msgid "Enables debug output"
msgstr "デバッグ出力を有効にする"

#: ../src/vncdisplay.c:2100
msgid "GTK-VNC Options:"
msgstr "GTK-VNC のオプション:"

#: ../src/vncdisplay.c:2100
msgid "Show GTK-VNC Options"
msgstr "GTK-VNC のオプションを表示する"
